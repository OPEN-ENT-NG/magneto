{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "title": {
      "type": "string"
    },
    "imageUrl": {
      "type": "string"
    },
    "backgroundUrl": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "folderId": {
      "type": ["string", "null"]
    },
    "public": {
      "type": "boolean"
    },
    "shared": {
      "type": "boolean"
    },
    "deleted": {
      "type": "boolean"
    },
    "layoutType": {
      "type": "string"
    },
    "canComment": {
      "type": "boolean"
    },
    "displayNbFavorites": {
      "type": "boolean"
    },
    "isLocked": {
      "type": "boolean"
    },
    "cardIds": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "type": "string"
          }
        ]
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "type": "string"
          }
        ]
      }
    },
    "isExternal": {
      "type": "boolean"
    },
    "sortOrCreateBy": {
      "type": "string",
      "enum": ["start", "end", "alphabetical", "anti-alphabetical", "newest-first", "oldest-first"]
    }
  },
  "required": ["title"]
}
